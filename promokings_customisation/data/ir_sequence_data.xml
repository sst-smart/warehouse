<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_promo_kings_manufacturing_order" model="ir.sequence">
            <field name="name">Customisation Manufacturing Order</field>
            <field name="code">customisation.manufacturing.order</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_order_next_action_plan" model="ir.sequence">
            <field name="name">Next Action Plan</field>
            <field name="code">next.action.plan</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_package_name_stock_picking" model="ir.sequence">
            <field name="name">New Package Name</field>
            <field name="code">next.package.name</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
